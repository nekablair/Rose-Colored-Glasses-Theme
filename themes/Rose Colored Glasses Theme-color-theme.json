{
	"name": "Rose Colored Glasses Theme",
	"type": "light",
	"colors": {
		"editor.background": "#FFF0F6",
		"editor.foreground": "#5A4C56",
		"editorCursor.foreground": "#E57373",
		"editorLineNumber.foreground": "#D1B2C7",
		"editorLineNumber.activeForeground": "#B56576",
		"editor.selectionBackground": "#F8DCE5",
		"sideBar.background": "#FDEAF6",
		"sideBar.foreground": "#815A68",
		"activityBar.background": "#F9DDE4",
		"activityBar.foreground": "#AA5776",
		"tab.activeBackground": "#F7D2E3",
		"tab.inactiveBackground": "#FCE9F2",
		"statusBar.background": "#FADDE3",
		"statusBar.foreground": "#8A6972"
	},
	"tokenColors": [
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#B48996"
			}
		},
		{
			"name": "Strings",
			"scope": "string",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#A55871"
			}
		},
		{
			"name": "Numbers",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#AB6576"
			}
		},
		{
			"name": "Keywords",
			"scope": "keyword",
			"settings": {
				"foreground": "#7A5479",
				"fontStyle": "italic bold"
			}
		},
		{
			"name": "Functions",
			"scope": "entity.name.function",
			"settings": {
				"foreground": "#C7658F",
				"fontStyle": "italic bold"
			}
		},
		{
			"name": "Variables",
			"scope": "variable",
			"settings": {
				"foreground": "#815A68"
			}
		},
		{
			"name": "Classes",
			"scope": [
				"entity.name.type",
				"entity.other.inherited-class"
			],
			"settings": {
				"foreground": "#C989A7",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Punctuation",
			"scope": "punctuation",
			"settings": {
				"foreground": "#D1B2C7"
			}
		},
		{
			"name": "Operators",
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#B5859E"
			}
		},
		{
			"name": "HTML: Tags",
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#E57373",
				"fontStyle": "bold"
			}
		},
		{
			"name": "HTML: Attribute Names",
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#D97399",
				"fontStyle": "italic"
			}
		}
	]
}